
  {
    "version": 2,
    "builds": [
      {
        "src": "Node/app.js", 
        "use": "@vercel/node"
      },
      {
        "src": "my-react/package.json", 
        "use": "@vercel/static-build", 
        "config": {
          "distDir": "my-react/build"
        }
      }
    ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Node/app.js"
    },
    {
      "src": "/(.*)", 
      "dest": "my-react/build"
    }
  ]
}
